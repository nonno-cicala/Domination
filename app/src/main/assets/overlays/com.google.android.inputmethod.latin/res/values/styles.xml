<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="AppThemeSelector" parent="@*android:style/Theme.Material" />

    <style name="BasePrivacyNotice">
        <item name="android:background">@*android:color/background_floating_material_dark</item>
        <item name="android:paddingTop">@*com.google.android.inputmethod.latin:dimen/privacy_notice_padding_top</item>
        <item name="android:paddingBottom">@*com.google.android.inputmethod.latin:dimen/privacy_notice_padding_bottom</item>
        <item name="android:clickable">true</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:paddingStart">24.0dip</item>
        <item name="android:paddingEnd">24.0dip</item>
    </style>
    <style name="BasePrivacyNoticeTitle">
        <item name="android:textSize">@*com.google.android.inputmethod.latin:dimen/privacy_notice_title_text_size</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingStart">@*com.google.android.inputmethod.latin:dimen/privacy_notice_title_padding_left</item>
        <item name="android:paddingEnd">@*com.google.android.inputmethod.latin:dimen/privacy_notice_title_padding_right</item>
    </style>

    <style name="CardViewerQueryText">
        <item name="android:textSize">@*com.google.android.inputmethod.latin:dimen/search_box_font_size</item>
        <item name="android:textColor">@*android:color/white</item>
        <item name="android:ellipsize">end</item>
        <item name="android:gravity">start|center</item>
        <item name="android:id">@*com.google.android.inputmethod.latin:id/card_viewer_query_text</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:maxLines">1</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="FirstRunPageDescription">
        <item name="android:textSize">@*com.google.android.inputmethod.latin:dimen/first_run_page_description_font_size</item>
        <item name="android:textColor">#ffdddddd</item>
        <item name="android:textColorLink">@*com.google.android.inputmethod.latin:color/first_run_page_blue_color</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@*com.google.android.inputmethod.latin:dimen/first_run_page_description_padding_top</item>
        <item name="android:lineSpacingMultiplier">1.2</item>
    </style>
    <style name="FirstRunPageFooter">
        <item name="android:layout_gravity">bottom</item>
        <item name="android:orientation">vertical</item>
        <item name="android:background">@*android:color/background_floating_material_dark</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">@*com.google.android.inputmethod.latin:dimen/first_run_page_footer_height</item>
    </style>

    <style name="FirstRunPagePermissionLabel">
        <item name="android:enabled">false</item>
        <item name="android:textSize">@*com.google.android.inputmethod.latin:dimen/first_run_page_description_font_size</item>
        <item name="android:textColor">#ffdddddd</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="FirstRunPageTitle">
        <item name="android:textSize">@*com.google.android.inputmethod.latin:dimen/first_run_page_title_font_size</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@*com.google.android.inputmethod.latin:dimen/first_run_page_title_padding_top</item>
    </style>

    <style name="FirstRunViewPager">
        <item name="android:background">@*android:color/background_floating_material_dark</item>
        <item name="android:paddingBottom">@*com.google.android.inputmethod.latin:dimen/first_run_page_footer_height</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
    </style>

    <style name="GBoardEditText">
        <item name="android:textSize">@*com.google.android.inputmethod.latin:dimen/search_box_font_size</item>
        <item name="android:textColor">@*android:color/white</item>
        <item name="android:ellipsize">end</item>
        <item name="android:background">@*android:color/transparent</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:maxLines">1</item>
        <item name="android:imeOptions">actionSearch</item>
    </style>

    <style name="LabelSecondary.Sub.Candidate" parent="@*com.google.android.inputmethod.latin:style/LabelSecondary.Sub">
        <item name="android:textColor">#ffdddddd</item>
        <item name="android:tag">.label.secondary</item>
    </style>

    <style name="Platform.AppCompat.Light" parent="@*android:style/Theme.Material" />


    <style name="PrivacyNoticeContentText">
        <item name="android:textSize">@*com.google.android.inputmethod.latin:dimen/privacy_notice_content_text_size</item>
        <item name="android:textColor">#ffdddddd</item>
        <item name="android:paddingBottom">@*com.google.android.inputmethod.latin:dimen/privacy_notice_content_text_padding_bottom</item>
        <item name="android:scrollbars">vertical</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">false</item>
        <item name="android:lineSpacingExtra">@*com.google.android.inputmethod.latin:dimen/privacy_notice_content_text_line_space_extra</item>
    </style>

    <style name="PrivacyNoticeLearnMore">
        <item name="android:textSize">@*com.google.android.inputmethod.latin:dimen/privacy_notice_learn_more_text_size</item>
        <item name="android:textColor">#ff1976d2</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="PrivacyNoticeRefuseButton" parent="@*com.google.android.inputmethod.latin:style/PrivacyNoticeButton">
        <item name="android:textColor">#ffdddddd</item>
        <item name="android:background">@*com.google.android.inputmethod.latin:drawable/bg_privacy_notice_refuse_button_selector</item>
        <item name="android:layout_marginEnd">@*com.google.android.inputmethod.latin:dimen/privacy_notice_refuse_button_margin_right</item>
    </style>
    <style name="SearchHeaderInnerView">
        <item name="android:orientation">vertical</item>
        <item name="android:id">@*com.google.android.inputmethod.latin:id/search_header_inner_view</item>
        <item name="android:background">@*android:color/primary_material_dark</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:divider">@*com.google.android.inputmethod.latin:drawable/search_box_divider</item>
        <item name="android:showDividers">middle</item>
    </style>

    <style name="SearchResultQueryTextView">
        <item name="android:textSize">@*com.google.android.inputmethod.latin:dimen/search_box_font_size</item>
        <item name="android:textColor">@*android:color/white</item>
        <item name="android:ellipsize">end</item>
        <item name="android:gravity">start|center</item>
        <item name="android:background">@*android:color/transparent</item>
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:maxLines">1</item>
        <item name="android:singleLine">true</item>
        <item name="android:layout_weight">1.0</item>
    </style>


    <style name="SettingsTheme" parent="@*android:style/Theme.Material" />
    <style name="SettingsTheme.Transparent" parent="@*android:style/Theme.Material.NoActionBar">
        <item name="android:windowBackground">@*android:color/transparent</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@null</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>

    <style name="SoftKeyCandidateSeparator.Popup" parent="@*com.google.android.inputmethod.latin:style/SoftKeyCandidateSeparator">
        <item name="android:tag"></item>
        <item name="android:src">@*android:color/background_floating_material_dark</item>
    </style>

    <style name="ThemeDetailsApplyButtonHolder">
        <item name="android:background">@*android:color/button_material_dark</item>
        <item name="android:paddingLeft">60.0dip</item>
        <item name="android:paddingTop">24.0dip</item>
        <item name="android:paddingRight">60.0dip</item>
        <item name="android:paddingBottom">48.0dip</item>
        <item name="android:clipToPadding">false</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="ThemeDetailsKeyBorderSwitch">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:id">@*com.google.android.inputmethod.latin:id/theme_details_switch_key_border</item>
        <item name="android:paddingLeft">12.0dip</item>
        <item name="android:paddingRight">12.0dip</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minHeight">48.0dip</item>
        <item name="android:text">@*com.google.android.inputmethod.latin:string/theme_details_button_title_key_border</item>
        <item name="android:switchPadding">12.0dip</item>
    </style>

    <style name="ThemeDetailsPreviewHolder">
        <item name="android:orientation">vertical</item>
        <item name="android:background">@*android:color/button_material_dark</item>
        <item name="android:paddingLeft">36.0dip</item>
        <item name="android:paddingTop">24.0dip</item>
        <item name="android:paddingRight">36.0dip</item>
        <item name="android:paddingBottom">24.0dip</item>
        <item name="android:clipToPadding">false</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="ThemeDetailsSwitchHolder">
        <item name="android:orientation">horizontal</item>
        <item name="android:background">@*android:color/background_floating_material_dark</item>
        <item name="android:paddingLeft">24.0dip</item>
        <item name="android:paddingTop">12.0dip</item>
        <item name="android:paddingRight">24.0dip</item>
        <item name="android:paddingBottom">12.0dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="TranslateNetworkStatus">
        <item name="android:textSize">@*com.google.android.inputmethod.latin:dimen/translate_board_editbox_font_size</item>
        <item name="android:textColor">@*com.google.android.inputmethod.latin:color/translate_board_text_idle_color</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:background">@*android:color/background_floating_material_dark</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:text">@*com.google.android.inputmethod.latin:string/translate_network_status_message</item>
        <item name="android:paddingStart">10.0dip</item>
    </style>
    <style name="TranslateQueryEditor">
        <item name="android:textSize">@*com.google.android.inputmethod.latin:dimen/translate_board_editbox_font_size</item>
        <item name="android:textColor">@*android:color/white</item>
        <item name="android:ellipsize">start</item>
        <item name="android:background">@*com.google.android.inputmethod.latin:drawable/bg_translate_editbox_corners</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:hint">@*com.google.android.inputmethod.latin:string/translate_query_editbox_hint</item>
        <item name="android:maxLines">1</item>
        <item name="android:maxLength">200</item>
        <item name="android:inputType">text</item>
        <item name="android:imeOptions">actionDone</item>
        <item name="android:paddingStart">10.0dip</item>
    </style>

    <style name="platformDialogTheme" parent="@*android:style/Theme.Material.Dialog" />


</resources>
